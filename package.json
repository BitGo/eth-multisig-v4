    "prettier-lint": "./node_modules/.bin/prettier --config .prettierrc --write './**/*.js' './**/*.ts'",
    "prettier:solidity": "./node_modules/.bin/prettier --write contracts/*.sol",
    "lint": "npm run prettier-check"
  },
  "keywords": [
    "multi-sig",
    "wallet",
    "ethereum"
  ],
  "dependencies": {
    "@openzeppelin/contracts": "4.4.1",
