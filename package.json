    "bluebird": "^3.3.5",
    "bn": "^1.0.1",
    "bn.js": "^4.11.3",
    "ethereumjs-abi": "^0.6.8",
    "ethereumjs-util": "^7.0.4",
    "ganache-cli": "^6.12.2",
    "lodash": "^4.12.0",
    "mocha": "^4.1.0",
    "q": "^1.5.1",
    "should": "^8.3.1",
    "solc": "0.8.11"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.4",
